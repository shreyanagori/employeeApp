<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.employee.controller.EmployeeView" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="Employee Information 3">
					<content>
					    <List noDataText="Drop list items here" id="list0" items="{/ContactSet}">
					        <items>
					            <StandardListItem type="Navigation" description="{EmailAddress}" icon="sap-icon://picture" id="item0" title="{FirstName}{LastName}"/>
					        </items>
					    </List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>